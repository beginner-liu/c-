# c-
从零开始
#define  _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>

int main()
{
	//ctrl+z
	//EOF=end of file->-1
	int ch = 0;
	while ((ch=getchar())!=EOF)
	{
		putchar(ch);
		printf("%c\n",ch);
	}
	return 0;
}







